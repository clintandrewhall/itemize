{
  "name": "itemize",
  "private": true,
  "version": "0.0.0",
  "description": "A TODO app to test Begin",
  "scripts": {
    "bnts": "npx babel-node-ts -b '@babel/preset-env,@babel/typescript'",
    "lint": "eslint lib --ext ts --ignore-pattern node_modules --fix",
    "clean": "rm -rf src/http && rm -rf build",
    "install:lib": "npm run bnts scripts/lib",
    "install:src": "npm run bnts scripts/src",
    "install": "npm run install:lib",
    "test:cra": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "rollup": "npx rollup -c && npm run install:src",
    "schema": "npm run bnts scripts/updateSchema",
    "relay": "relay-compiler --language typescript --extensions ts tsx",
    "build:lib": "npm run clean && npm run install:lib && npm run rollup && npm run relay",
    "build:cra": "react-app-rewired build",
    "build": "npm run build:lib && npm run build:cra",
    "watch:lib": "npx rollup -c -w",
    "watch:relay": "npm run relay -- --watch",
    "watch:cra": "react-app-rewired start",
    "watch:sandbox": "cross-env NODE_ENV=testing ARC_LOCAL=1 npx sandbox",
    "watch:app": "concurrently \"npm run watch:sandbox\" \"npm run watch:cra\"",
    "watch": "concurrently \"npm run watch:lib\" \"npm run watch:relay\" \"npm run watch:app\"",
    "start": "npm run build:lib && npm run watch:app",
    "serve": "npm run build && npm watch:sandbox"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^9.0.0"
  },
  "devDependencies": {
    "@architect/architect": "latest",
    "@architect/sandbox": "latest",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/graphql-relay": "^0.6.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-relay": "^7.0.3",
    "@types/relay-runtime": "^8.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "babel-node-ts": "^1.0.3",
    "babel-plugin-relay": "^9.0.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "bufferutil": "^4.0.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^0.9.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-relay": "^1.7.0",
    "fibers": "^4.0.2",
    "graphql": "^14.6.0",
    "graphql-compiler": "^1.7.0",
    "graphql-relay": "^0.6.0",
    "http-proxy-middleware": "^1.0.3",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "3.4.1",
    "relay-compiler": "^9.0.0",
    "relay-compiler-language-typescript": "^12.0.0",
    "relay-config": "^9.0.0",
    "relay-runtime": "^9.0.0",
    "rollup": "^2.2.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-graphql": "^0.1.0",
    "sass": "^1.26.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "typescript-plugin-css-modules": "^2.2.0",
    "utf-8-validate": "^5.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
